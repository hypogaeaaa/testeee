<script>
var dropdown = $('#concurso');
for(c=2706;c>0;c--){ dropdown.append($('<option></option>').attr('value', c).text(c)); }
dropdown.val('0');
dropdown.prop( "disabled",false);
</script>
<script>
conf();

function conf(){

m = document.getElementById("meujogo").value;
o = document.querySelector('input[name="order"]:checked').value;
c = document.getElementById("concurso").value;
$("#conf-r-2").html("<div class='conf-load'>Carregando...</div>");
	$.getJSON('confere.php?meujogo='+m+'&concurso='+c+'&order='+o+'&cmd=load', function (data) {
		$("#conf-r-2").html("");
		$("#acz").html("");
		$.each(data['li'], function (key,entry) {
		c = ''; p = '';
		if(entry.a >= 11) c =  ' resBg';
		if(entry.a >= 15) c =  ' resG';
		if(entry.p != null) p =  ' &nbsp ' + entry.p;
		$("#conf-r-2").append('<div class="res'+c+'"><b>Concurso:</b><a href="resultado.php?concurso=' + entry.c + '" target="_blank">' + entry.c + '</a> <b>Total Acertos:</b> ' + entry.a + p + '</div>'); })
		$.each(data['ac'], function (key,entry) {
			if(key >= 11)
			$("#acz").append(key+' Acertos: '+entry+'<br>');
		})
		$("#act1").html(data['at']);
		$("#act2").html(data['at']);
		$("#acp").html(data['ap']);
}); }
</script>
<script>
var min = 15;
var max = 20;
function post(cmd){	
	f = document.forms['loto'];
	f.cmd.value = cmd;
		var dez = getDez();
	if(dez.length < 1) {
		$("#aviso").html("Selecione 15 dezenas");
		$("#aviso").addClass("alert alert-danger");
		return false;
	}
	dz = dez.join(',');
	$("#meujogo").val(dz);
	url = "?meujogo="+dz;
	window.history.replaceState("object", "MazuSoft", url);
		if(cmd == 'save') {
		$("#aviso").html("Efetue seu Login para Salvar");
		$("#aviso").addClass("alert alert-danger");
		return false;
	}
		f.submit();
}
</script>

